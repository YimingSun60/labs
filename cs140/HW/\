#include <iostream>
#include <cstdlib>
#include <cmath>
using namespace std;
int factorial(int &number){
int i=1;
int temp=1;
i++;


if(i<number){
factorial(number);
}

return temp;
}

int main(int argc, char*argv[]){
	int n=atoi(argv[1]);
	int factorial_n=factorial(n);
	cout<<"factorial="<<factorial_n<<"\n";
}
